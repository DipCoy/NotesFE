@{
    Layout = "_layout";
}

<div>
    <label>StickerText:</label><br />
    
    <form method="post">
        <p><input name="boardModel.accessType" type="radio" value="nedzen"> Не дзен</p>
        <p><input name="boardModel.accessType" type="radio" value="dzen"> Дзен</p>
        <p><input name="boardModel.accessType" type="radio" value="pdzen" checked> Полный дзен</p>
        <div>
            <button type="button" id="add_input" class="btn btn-primary btn-lg active" onclick="add_btn();">Добавить стикер</button>
        </div>
    
        <div class="mp-2">
            <button type="submit" class="btn btn-success">Submit</button>
        </div>
    </form>
    <script src="js/script.js"></script>
</div>